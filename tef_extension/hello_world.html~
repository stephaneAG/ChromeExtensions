				  <script type="text/javascript">
				   function open_tab(url)
				   {
				    chrome.tabs.create({"url": url});
				   }
				  </script>
				 </head>
				 <body>
				  <ul>
				   <li><a href="" onclick="open_tab('http://www.google.fr')">Google</a></li>
				   <li><a href="" onclick="open_tab('http://mail.google.com')">Gmail</a></li>
